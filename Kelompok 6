void ground() {
    //pondasi
    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.2f, 0.2f);
    glVertex3f(-1000.0, -5.0, -1000.0);
    glVertex3f(1000.0, -5.0, -1000.0);
    glVertex3f(1000.0, -5.0, 1000.0);
    glVertex3f(-1000.0, -5.0, 1000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.2f, 0.2f);
    glVertex3f(-1000.0, 0, -1000.0);
    glVertex3f(1000.0, 0, -1000.0);
    glVertex3f(1000.0, -300, -1000.0);
    glVertex3f(-1000.0, -300, -1000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.2f, 0.2f);
    glVertex3f(1000.0, 0, 1000.0);
    glVertex3f(-1000.0, 0, 1000.0);
    glVertex3f(-1000.0, -300, 1000.0);
    glVertex3f(1000.0, -300, 1000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.2f, 0.2f);
    glVertex3f(-1000.0, 0, -1000.0);
    glVertex3f(-1000.0, 0, 1000.0);
    glVertex3f(-1000.0, -300, 1000.0);
    glVertex3f(-1000.0, -300, -1000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.2f, 0.2f);
    glVertex3f(1000.0, 0, -1000.0);
    glVertex3f(1000.0, 0, 1000.0);
    glVertex3f(1000.0, -300, 1000.0);
    glVertex3f(1000.0, -300, -1000.0);
    glEnd();

    //jalan-rumput
    glBegin(GL_QUADS);
    glColor3f(0.0f, 0.24f, 0.09f);
    glVertex3f(2000.0, -299.1, 3000.0);
    glVertex3f(-2000.0, -299.1, 3000.0);
    glVertex3f(-2000.0, -299.1, -5000.0);
    glVertex3f(2000.0, -299.1, -5000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.34f, 0.35f, 0.36f);
    glVertex3f(1000.0, -298, 3000.0);
    glVertex3f(-1000.0, -298, 3000.0);
    glVertex3f(-1000.0, -298, -5000.0);
    glVertex3f(1000.0, -298, -5000.0);
    glEnd();

    //jurangmania
    glBegin(GL_QUADS);
    glColor3f(0.37, 0.27, 0.27);
    glVertex3f(-2000.0, -299.1, 3000.0);
    glVertex3f(-2000.0, -299.1, -5000.0);
    glVertex3f(-2000.0, -2000.1, -5000.0);
    glVertex3f(-2000.0, -2000.1, 3000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.39f, 0.25f, 0.25f);
    glVertex3f(-4000.0, 600.1, 1500.0);
    glVertex3f(-4000.0, 600.1, -5000.0);
    glVertex3f(-4000.0, -2000.1, -5000.0);
    glVertex3f(-4000.0, -2000.1, 1500.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.39f, 0.25f, 0.25f);
    glVertex3f(-4000.0, 600.1, 1500.0);
    glVertex3f(-4000.0, 600.1, -5000.0);
    glVertex3f(-6000.0, 600.1, -5000.0);
    glVertex3f(-6000.0, 600.1, 1500.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.18f, 0.58f, 0.82f);
    glVertex3f(-2000.0, -2000.1, -5000.0);
    glVertex3f(-2000.0, -2000.1, 3000.0);
    glVertex3f(-4000.0, -2000.1, 3000.0);
    glVertex3f(-4000.0, -2000.1, -5000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.18f, 0.58f, 0.82f);
    glVertex3f(-4000.0, 600.1, 1500.0);
    glVertex3f(-4000.0, -2000.1, 1500.0);
    glVertex3f(-4000.0, -2000.1, 3000.0);
    glVertex3f(-4000.0, 600.1, 3000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.18f, 0.58f, 0.82f);
    glVertex3f(-4000.0, 600.1, 1500.0);
    glVertex3f(-4000.0, 600.1, 3000.0);
    glVertex3f(-6000.0, 600.1, 3000.0);
    glVertex3f(-6000.0, 600.1, 1500.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.1f, 0.0f);
    glVertex3f(-4000.0, -2000.1, 3000.0);
    glVertex3f(-4000.0, 600.1, 3000.0);
    glVertex3f(2000.0, 600.1, 3000.0);
    glVertex3f(2000.0, -2000.1, 3000.0);
    glEnd();

    glBegin(GL_QUADS);
    glColor3f(0.2f, 0.1f, 0.1f);
    glVertex3f(-6000.0, 600.1, 3000.0);
    glVertex3f(2000.0, 600.1, 3000.0);
    glVertex3f(2000.0, 1500.1, 3500.0);
    glVertex3f(-6000.0, 1500.1, 3500.0);
    glEnd();

    //pagar lantai dasar
   //kiri

    pagarlantaidasar();

    //kanan
    glPushMatrix();
    glTranslatef(1995, 0, 0);
    pagarlantaidasar();
    glPopMatrix();

    //blakang
    glPushMatrix();
    glTranslatef(0, 0, 0);
    glRotatef(90, 0, 60, 0);
    pagarlantaidasar();
    glPopMatrix();

    //depan kiri
    glPushMatrix();
    glTranslatef(-595, 0, -1995);
    glScalef(0.4, 1, 1);
    glRotatef(90, 0, 60, 0);
    pagarlantaidasar();
    glPopMatrix();

    //depan kanan
    glPushMatrix();
    glTranslatef(595, 0, -1995);
    glScalef(0.4, 1, 1);
    glRotatef(90, 0, 60, 0);
    pagarlantaidasar();
    glPopMatrix();



    //pagar lantai 1
    //blakang
    glPushMatrix();
    glTranslatef(0, 400, 0);
    glRotatef(90, 0, 60, 0);
    pagar();
    glPopMatrix();


    //depan
    glPushMatrix();
    glTranslatef(0, 400, -2000);
    glRotatef(90, 0, 60, 0);
    pagar();
    glPopMatrix();


    //kiri
    glPushMatrix();
    glTranslatef(0, 400, 0);
    pagar();
    glPopMatrix();

    //kanan
    glPushMatrix();
    glTranslatef(1995, 400, 0);
    pagar();
    glPopMatrix();


    //lantai 2
    //kiri
    glPushMatrix();
    glTranslatef(480, 950, 0);
    glScalef(1, 0.8, 0.52);
    pagar();
    glPopMatrix();

    //kanan
    glPushMatrix();
    glTranslatef(1316, 950, 0);
    glScalef(0.8, 0.8, 0.52);
    pagar();
    glPopMatrix();


    //depan
    glPushMatrix();
    glTranslatef(5, 950, -1040);
    glScalef(0.52, 0.8, 0.52);
    glRotatef(90, 0, 60, 0);
    pagar();
    glPopMatrix();

    //blakang
    glPushMatrix();
    glTranslatef(0, 950, 0);
    glScalef(0.52, 0.8, 0.52);
    glRotatef(90, 0, 60, 0);
    pagar();
    glPopMatrix();

    //Lantai 3

    glPushMatrix();
    glTranslatef(480, 950 + 270, 0);
    glScalef(1, 0.8, 0.52);
    pagar();
    glPopMatrix();

    //kanan
    glPushMatrix();
    glTranslatef(1316, 950 + 270, 0);
    glScalef(0.8, 0.8, 0.52);
    pagar();
    glPopMatrix();


    //depan
    glPushMatrix();
    glTranslatef(5, 950 + 270, -1040);
    glScalef(0.52, 0.8, 0.52);
    glRotatef(90, 0, 60, 0);
    pagar();
    glPopMatrix();

    //blakang
    glPushMatrix();
    glTranslatef(0, 950 + 270, 0);
    glScalef(0.52, 0.8, 0.52);
    glRotatef(90, 0, 60, 0);
    pagar();
    glPopMatrix();

}

void door(int sisi, int x, int y, int z) {
    glBegin(GL_POLYGON);
    int r = 20;
    for (int i = 0; i < sisi; i++) {
        glVertex3f((r * cos(2 * 3.14159265 * i / sisi)) + x, (r * sin(2 * 3.14159265 * i / sisi)) + y, z);
    }
    glEnd();

}

