    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();// line 4
