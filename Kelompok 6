void lampu() {

    // LAMPU
    glPushMatrix(); // silinder lampu
    glTranslatef(-14, 339.5, 3.5);
    glRotatef(90.0, 50, 0, 1.0);
    glColor3f(0.33f, 0.31f, 0.31f);
    gluCylinder(gluNewQuadric(), 45.0f, 45.0f, 20.0f, 32, 45);
    glPopMatrix();

    glPushMatrix(); //line1
    glTranslatef(-369.5, -71, 399.5);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix(); //line2
    glTranslatef(-373.5, -71, 420.5);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();//line 3
    glTranslatef(-385.5, -71, 437.5);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix(); // lampu
    glTranslatef(-14, 327.5, -2);
    glColor3f(1.0f, 0.92f, 0.0f);
    glutSolidSphere(20.0, 30, 30);
    glPopMatrix();

    glPushMatrix();// line lampu
    glTranslatef(-416.5, -71, 391);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();// line 3
    glTranslatef(-416.5, -71, 447.5);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();// line 4
    glTranslatef(-441, -71, 438);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();// line 5
    glTranslatef(-458.5, -71, 399);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();// line 6
    glTranslatef(-440.9, -71, 368.1);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();// line 7
    glTranslatef(-407, -71, 359.7);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    //Lantai 2

    glPushMatrix(); // silinder lampu
    glTranslatef(-14, 899, 3.5);
    glRotatef(90.0, 50, 0, 1.0);
    glColor3f(0.33f, 0.31f, 0.31f);
    gluCylinder(gluNewQuadric(), 45.0f, 45.0f, 20.0f, 32, 45);
    glPopMatrix();

    glPushMatrix(); //line1
    glTranslatef(-369.5, 478, 399.5);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix(); //line2
    glTranslatef(-373.5, 478, 420.5);
    glLineWidth(4);
    glColor3f(0.0f, 0.31f, 0.31f);
    glBegin(GL_LINES);
    glVertex3f(400.0, 402, -400.0);
    glVertex3f(400.0, 472, -400.0);
    glEnd();
    glPopMatrix();

    glPushMatrix();//line 3
