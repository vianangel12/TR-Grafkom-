    glColor3f(0.41f, 0.2f, 0.09f);
    glTranslatef(1510, -199.9, -2710);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix();
    glColor3f(0.41f, 0.2f, 0.09f);
    glTranslatef(1510, -99.9, -2710);
    glutSolidCube(100.0);
    glPopMatrix();

    //daun
    glPushMatrix(); //depan
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1510, -99.9, -2810);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix(); //depan kanan
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1610, -99.9, -2810);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix(); //depan kiri
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1410, -99.9, -2810);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix(); //tengah kanan
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1610, -99.9, -2710);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix(); //tengah kiri
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1410, -99.9, -2710);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix();//belakang
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1510, -99.9, -2610);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix();//belakang kanan
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1610, -99.9, -2610);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix();//belakang kiri
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1410, -99.9, -2610);
    glutSolidCube(100.0);
    glPopMatrix();

    glPushMatrix(); //belakang kiri
    glColor3f(0.0f, 0.46f, 0.03f);
    glTranslatef(1510, -0.9, -2710);
    glutSolidCube(100.0);
    glPopMatrix();
}

